{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "ESNext",
    "lib": ["ESNext", "DOM"],
    "types": ["@types/node"]
  },
  "include": [".nuxt", "nuxt.config.ts", "app.vue", "**/*.vue", "**/*.ts"]
}
